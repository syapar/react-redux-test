  "private": true,
  "dependencies": {
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "build-css": "lessc src/style/index.less public/index.css",
